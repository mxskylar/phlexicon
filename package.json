{
    "name": "phlexicon",
    "version": "0.0.0",
    "description": "A phonetic catalog for lexicons: Study a language. Document vocabulary. Analyze accents.",
    "main": "build/main.js",
    "author": "https://github.com/mxskylar",
    "license": "MIT",
    "scripts": {
        "install-theme": "curl -o resources/bootstrap.min.css https://bootswatch.com/5/cerulean/bootstrap.min.css",
        "install-js": "curl -o resources/bootstrap.bundle.min.js https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js",
        "postinstall": "npm run install-js && npm run install-theme",
        "compile-react": "webpack --mode development --config webpack.react.config.js",
        "compile": "rm -rf build && cp -r resources build && tsc && npm run compile-react",
        "start": "npm run compile && electron ."
    },
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router-dom": "^7.4.0"
    },
    "devDependencies": {
        "electron": "^35.0.3",
        "typescript": "^5.8.2",
        "@types/react-dom": "^19.0.4",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1",
        "html-webpack-plugin": "^5.6.3",
        "babel-loader": "^10.0.0",
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.10",
        "@babel/plugin-transform-class-properties": "^7.18.6",
        "@babel/plugin-transform-arrow-functions": "^7.25.9",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-typescript": "^7.26.0"
    }
}
